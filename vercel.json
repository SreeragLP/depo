{
    "builds": [
      {
        "src": "vihicle/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9",
          "builds": [{ "src": "./build.sh", "use": "@now/static-build", "config": {"distDir": "build"} }]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "vihicle/wsgi.py"
      }
    ]
  }
  