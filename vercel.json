{
    "builds": [{
        "src": "vihicle/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
        "buildCommand" : "./install.sh"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vihicle/wsgi.py"
        }
    ]
}